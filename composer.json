{
	"name": "lykegenes/laravel-datagrid-builder",
	"description": "Laravel datagrid generator - using jQuery-bootgrid",
	"keywords": [
		"laravel",
		"bootgrid",
		"datagrid",
		"table",
		"datatable"
	],
	"homepage": "https://github.com/Lykegenes/laravel-datagrid-builder",
	"license": "MIT",
	"authors": [
		{
			"name": "Patrick Samson",
			"email": "patricksamson236@gmail.com"
		}
	],
	"require": {
		"illuminate/support": "~5.1",
		"illuminate/http": "~5.1",
        "league/fractal": "^0.12.0",
        "sofa/eloquence": "~5.1",
		"php": ">=5.4.0"
	},
	"require-dev": {
		"phpunit/phpunit" : "4.*"
	},
	"autoload": {
		"psr-4": {
			"Lykegenes\\DatagridBuilder\\": "src"
		},
		"files": [
			"src/RenderingHelpers.php"
		]
	},
	"scripts": {
		"test": "phpunit"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
